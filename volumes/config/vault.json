{
  "api_addr": "vault:8200",
  "log_level": "Info",
  "backend": {
    "file": {
      "path": "/vault/file"
    }
  },
  "listener": {
    "tcp":{
      "address": "0.0.0.0:8200",
      "tls_disable": 0,
      "tls_cert_file": "/vault/config/vault_public.pem",
      "tls_key_file": "/vault/config/vault_key.pem",
      "tls_min_version": "tls12"
    }
  },
  "ui": true
}
